package classtest5;

import java.util.Scanner;

public class vowels {

	public static void main(String[] args) {
		String name;
		System.out.println("Enter string");
		Scanner sc=new Scanner(System.in);
		name=sc.nextLine();
		int len=name.length();
		char a[]=new char[len];
		int vow=0;
		for(int i=0;i<len;i++)
		{
		 a[i]=name.charAt(i);
		 if(a[i]=='a'||a[i]=='e'||a[i]=='i'||a[i]=='o'||a[i]=='u'
		 ||a[i]=='A'||a[i]=='E'||a[i]=='I'||a[i]=='O'||a[i]=='U')
		 vow=vow+1;
		}
		System.out.println("number of vowels present in the string is"+vow);

	}

}
